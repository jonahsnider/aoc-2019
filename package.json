{
	"author": {
		"email": "jonah@jonah.pw",
		"name": "Jonah Snider",
		"url": "https://jonah.pw"
	},
	"ava": {
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"files": [
			"!tsc_output"
		],
		"require": [
			"ts-node/register",
			"source-map-support/register"
		]
	},
	"bugs": {
		"url": "https://github.com/pizzafox/aoc-2019/issues"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.1",
		"ava": "^2.4.0",
		"nyc": "^15.0.1",
		"parcel": "^1.12.4",
		"prettier": "^2.0.2",
		"source-map-support": "^0.5.16",
		"ts-node": "^8.6.2",
		"typescript": "^3.7.5",
		"xo": "^0.27.2"
	},
	"engines": {
		"node": "12"
	},
	"license": "Apache-2.0",
	"main": "./tsc_output/src/index.js",
	"name": "aoc-2019",
	"nyc": {
		"all": true,
		"extends": "@istanbuljs/nyc-config-typescript",
		"include": [
			"src/**/*.{t,j}s"
		],
		"reporter": [
			"lcov",
			"cobertura"
		]
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pizzafox/aoc-2019.git"
	},
	"scripts": {
		"build": "tsc",
		"lint": "xo",
		"prebuild": "rm -rf tsc_output",
		"pretest": "rm -rf coverage .nyc_output",
		"style": "prettier --ignore-path .gitignore --check \"./**/*.{{j,t}{s,sx},flow,{sc,c,le}ss,json,m{d,dx},{y,ya}ml}\"",
		"test": "nyc ava",
		"test:no-cov": "ava"
	},
	"version": "1.0.0",
	"xo": {
		"ignores": [
			"src/*/performance.js"
		],
		"prettier": true
	}
}
